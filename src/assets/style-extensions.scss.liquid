      gap: 2em;
      justify-content: space-evenly;
      margin-bottom: 10px;
      .bonus-tiered-product {
        .product-image-container {
          max-width: 100px;
          margin: auto;
        }
      }
      .bonus-tiered-title {
        padding: 4px 10px;
        text-align: center;
        font-weight: bold;
        a {
          font-size: 14px;
          color:#16142C;
        }
      }
      .original-price {
        text-decoration: line-through;
      }
    }
  }
  .cartRewardLevel {
    font-weight: bold;
  }
  .bonusRewards-progress {
    transition: all 0.3s;
    width: 100%;
    background-color: #c4cdd5;
    border-radius: 5px;
  }
}

/*------------------------------------------------------------------
LIST COLLECTION
-------------------------------------------------------------------*/
.template-list-collections {
  #content {
    padding: 0;
    .collections-container {
      display: flex;
      flex-wrap: wrap;
      justify-content: space-around;
      row-gap: 30px;
      margin-bottom: 30px;
    }
    .product_c {
      .collection-container {
        flex-basis: 31%;
        border: 1px solid #3dbfde;
        box-shadow: 0 3px 3px #d1d7d7;
        transition: all 0.3s;
        @media (max-width: 768px) {
          flex-basis: 46%;
        }
        @media (max-width: 480px) {
          flex-basis: 100%;
        }
        a {
          display: flex;
          height: 100%;
          flex-direction: column;
          justify-content: space-between;
        }
        .box_1 {
          height: 200px;
          display: flex;
          justify-content: center;
          align-content: center;
          background-color: white;
          a {
            display: flex;
            img {
              object-fit: contain;
            }
          }
        }
        .desc {
          margin: 10px 25px;
          padding: 5px 0;
          background-color: transparent;
          border-bottom: 1px solid lightgray;
          .item-count {
            color: #16142C;
          }
        }
        .collection-title {
          text-transform: uppercase;
          font-size: 1.1em;
          font-weight: 600;
          color: #107fa8;
        }
        .collection-button {
          display: block;
          color: #16142c;
          background-color: #3dbfde;
          padding: 0.5em 1.5em;
          font-size: 1em;
          font-weight: 700;
          text-align: center;
          text-transform: uppercase;
          text-decoration: none;
          transition: all .3s;
        }
        &:hover {
          transform: scale(1.05);
          .collection-button {
            background-color: #77dbf3;
          }
        }
        &:focus {
          text-decoration: unset;
        }
      }
    }
  }
}

/*------------------------------------------------------------------
FEATURED PRODUCTS
-------------------------------------------------------------------*/
#featuredProduct {
  width: 85%;
  margin: auto;
  margin-bottom: 30px;
  border: 1px solid #c4cdd5;
  box-shadow: 3px 3px 5px #c4cdd5;
  .section-title {
    color: #16142c;
    background-color: #ffda57;
    padding: 5px 10px;
    font-size: 18px;
    font-weight: 600;
  }
  .featured-product-wrapper {
    padding: 10px;
    h3 {
      text-transform: uppercase;
      a {
        color: #16142c;
      }
    }
    #image-block .product_gallery .product_slides li a {
      height: 200px;
      @media (max-width: 991px) { 
        height: 150px;
      }
      @media (max-width: 560px) { 
        height: 110px;
      }
    }
    .product-summary-container {
      position: initial;
    }
  }
  .product-summary-container input#addToCart.btn_c {
    margin: auto;
    margin-top: 10px;
  }
}

