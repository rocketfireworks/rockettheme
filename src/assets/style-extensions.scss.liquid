/*------------------------------------------------------------------
MIXINS
-------------------------------------------------------------------*/
@mixin line-clamp($numLines : 1, $lineHeight: 1) {
  overflow: hidden;
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  white-space: initial;
  display: block;
  /* autoprefixer: off */
  display: -webkit-box;
  -webkit-line-clamp: $numLines;
  -webkit-box-orient: vertical;
  max-height: $numLines * $lineHeight + unquote('em');
}

/*------------------------------------------------------------------
General
-------------------------------------------------------------------*/
html {
  scroll-behavior: smooth;
}
.text-dark {
  color: #16142C;
  p, li {
    color: #16142C;
  }
}
.text-important {
  color: #C94344;
}
.text-uppercase {
  text-transform: uppercase;
}
.linethrough {
  text-decoration: line-through;
}
.layout-inline {
  display: flex;
  align-items: center;
  &>:first-child {
  	margin-right: 10px;
  }
}
.disabled {
  pointer-events: none;
}
// BUTTONS
.btn_c, .next_btb {
  border-radius: 0;
  padding: 0.3em 1em;
  color: #16142c;
  text-transform: uppercase;
  background-color: #3cc0df;
  &:hover, &:focus {
    text-decoration: none;
    color: #16142c;
    background-color: #77dbf3;
  }
}
.btn_c.floatCCartBtn,
#content .cart_btn button.next_btn {
  background-color: #3dbfde;
  color: #16142c;
  text-transform: uppercase;
  font-size: 20px;
  max-width: 300px;
  border-radius: 0;
  padding: 0.3em 1em;
  &:hover, &:focus {
    text-decoration: none;
    background-color: #77dbf3;
  }
  
}

.product-image-link {
  display: flex;
  img {
    width: 100%;
    margin: 0 auto;
    object-fit: contain;
  }
}

// ERRORS
.error-popup {
  position: absolute;
  width: max-content;
  background-color: #ffda57;
  color: black;
  box-shadow: 1px 1px 3px #ccc;
  padding: 4px 10px;
}

.photobox {
  border-radius: 30px;
/*   border: 3px solid #ffda57; */
  box-shadow: 3px 3px 3px #d1d7d7;
  margin-bottom: 5px;
}
.rte .contact-page a:hover {
  text-decoration: underline !important;
}
.rte .ancillary-page a:hover {
  text-decoration: underline !important;
}
.videoWrapper {
  position: relative;
  padding-bottom: 56.25%; /* 16:9 */
  height: 0;
}
.videoWrapper iframe {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.intro-p {
  margin-left: 15px;
  margin-right: 15px;
}
.icon {
  display: inline-block;
  width: 28px;
  height: 28px;
  vertical-align: middle;
}
.icon-fireworks-1 {
  background-image: url({{ 'fireworks-icon-1.svg' | asset_url }});
  background-size: contain;
  background-repeat: no-repeat;
}
.icon-fireworks-2 {
  background-image: url({{ 'fireworks-icon-2.svg' | asset_url }});
  background-size: contain;
  background-repeat: no-repeat;
}
.load-more__icon {
  content: '';
  box-sizing: border-box;
  top: 50%;
  left: 50%;
  width: 30px;
  height: 30px;
  margin-top: -10px;
  margin-left: -10px;
  border-radius: 50%;
  border: 2px solid #ccc;
  border-top-color: #333;
  animation: spinner .6s linear infinite;
  display: none;
}
.loading-in-progress + .load-more__icon {
  display: block;
}
.video-container {
  opacity: 1;
}
/*------------------------------------------------------------------
Custom Page Template
-------------------------------------------------------------------*/

.template-page #content {
  padding-top: 0;
}
.custom-container {
  &.rte {
    padding: 0;
    p, div, li {
      color: #16142c;
    }
  }
  .custom-title {
    display: none;
  }
  .checkmark {
    li {
      list-style: none;
      font-weight: bold;
      font-size: 18px;
      i {
        margin-right: 5px;
      }
    }
  }
}
.container-narrow {
  position: relative;
  margin-left: auto;
  margin-right: auto;
  padding: 0 20px;
  @media (max-width: 768px) and (min-width: 480px) {
    max-width: 748px;
  }
  @media (min-width: 768px) {
    width: 750px;
  }
  @media (min-width: 992px) {
    width: 970px;
  }
  @media (min-width: 1200px) {
    width: 100%;
    max-width: 1200px;
  }
}

/*------------------------------------------------------------------
Location Page
-------------------------------------------------------------------*/
.location-master {
  p {
    color: #16142C;
  }
  ul, ol {
    margin-top: 10px
  }
  li {
    color: #16142C;
  }
  .location-summary {
    display: flex;
    justify-content: space-around;
    align-items: center;
    flex-flow: wrap;
    &>:first-child {
      padding: 0 20px 20px 20px;
      flex: 50%;
    }
    &>:last-child {
      flex: 20%;
    }
  }
  .reward {    
    display: flex;
    align-items: center;
    border: 0.5px solid #d1d7d7;
    padding: 10px 15px;
    .fas {
      font-size: 40px;
      margin-right: 10px;
    }
    p:last-child {
      margin-bottom: 0;
    }
  }
  h2 {
    text-align: center;
    font-size: 22px;
  }
  .location-featured, .location-normal {
    display:flex;
    justify-content: space-around;
    flex-flow: wrap;
    &>a {
      box-shadow: 0 3px 3px #d1d7d7;
      margin: 20px 0;
      border: 0.5px solid #d1d7d7;
      transition: all 0.3s;
      &>:last-child {
        margin-bottom: 15px;
      }
      &:hover, &:focus {
        background-color: #eae8e8;
        text-decoration: none;
        transform: scale(1.05);
      }
    }
    h3 {
      border-bottom: 0.5px solid #d1d7d7;
      margin-bottom: 0;
      color: #107fa8;
    }
    i {
      width: 16px;
    }
    div {
      color: #16142C;
    }
    .location-contact {
      margin: 10px 0;
    }
    .location-address {
      font-weight: bold;
    }
  }
  .location-featured {
    margin: 15px 0 30px;
    font-size: 16px;
    &>a {
      flex: 0 0 40%;
      border: 1px solid #107fa8;
      box-shadow: 0 3px 3px rgba(16, 127, 168, 0.75);
      h3 {
        font-size: 22px;
        background-color: #107fa8;
        color: white;
        margin: 0;
        padding: 10px 25px 0 25px;
      }
      &>div, &>p {
        padding: 0 25px;
      }
      .location-status {
/*         color: #C94344; */
        color: white;
        background-color: #c94344;
        font-weight: bold;
      }
    }
  }
  .location-normal {
    &>a {
      flex:0 0 32%;
      padding: 0 25px;
    }
  }
  .location-address-comment {
    padding-left: 20px;
  }
  @media (max-width: 1026px) {
    .location-normal, .location-normal {
      &>a {
        flex:0 0 49%;
      }
    }
  }
  @media (max-width: 640px) {
    .location-featured, .location-normal {
      margin: 30px 0;
      &>a {
        flex: 0 0 100%;
      }
    }
  }
}
.location-page {
  display: grid;
  grid-template-columns: 40% 320px;
  // grid-template-rows: auto 1fr auto;
  column-gap: 5%;
  justify-content: center;
  justify-items: start;
  h1 {
    font-size: 26px;
    margin-top: 0;
    grid-column: 1 / 3;
    grid-row: 1 / 2;
  }
  .location-general {
    grid-column: 1 / 2;
    grid-row: 2 / 3;
    a {
      color: #16142C;
      &:hover {
        text-decoration: underline !important;
      }
    }
    i {
      width: 18px;
    }
  }
  .location-info {
    grid-column: 1 / 2;
    grid-row: 3 / 4;
  }
  .location-image {
    grid-column: 2 / 3;
    grid-row: 2 / 3;
    width: 100%;
  }
  .location-map {
    grid-row: 3 / 4;
    grid-column: 2 / 3;
  }
  @media (max-width: 768px) {
    grid-template-columns: 1fr;
    column-gap: 0;
    h1 {
      grid-row: 1 / 2;
      grid-column: 1 / 3;
    }
    .location-image {
      grid-row: 2 / 3;
      grid-column: 1 / 3;
      width: 60%;
    }
    .location-general {
      grid-row: 3 / 4;
      grid-column: 1 / 3;
      width: 100%;
    }
    .location-map {
      grid-row: 4 / 5;
      grid-column: 1 / 3;
    }
    .location-info {
      grid-row: 5 / 6;
      grid-column: 1 / 3;
    }
  }
  h2 {
    text-transform: uppercase;
    border-bottom: 0.5px solid #898989;
  }
  p {
    color: #16142C;
  }
  ul, ol {
    margin-top: 10px
  }
  li {
    color: #16142C;
  }
  .announcement {
    font-weight: bold;
  }
  div.seasonal {
    font-size: 18px;
    font-weight: bold;
    line-height: 35px;
    color: #107fa8;
    margin-bottom: 10px;
  }
  .basic-info {
    font-size:15px;
    margin-bottom: 10px;
  }
  .basic-info .address {
    text-transform: uppercase;
  }
  .basic-info .comment {
    font-size: 13px;
    padding-left: 18px;
  }
  div.open-status {
    margin: 10px 0;
    font-weight: bold;
    font-size: 20px;
    color: #C94344;
    text-transform: uppercase;
  }
  #hours, #customHours {
    div, p {
      margin: 10px 0;
      span {
        float: right;
      }
    }
  }
}
/*------------------------------------------------------------------
Ancillary Page
-------------------------------------------------------------------*/
.ancillary-page {
  p {
    color: #16142C;
  }
  ul, ol {
    margin-top: 10px
  }
  li {
    color: #16142C;
  }
  .float-clear {
  	clear: both;
  }
  .text-important {
    color: #C94344;
  }
  .link-as-button {
    background-color: #3DBFDE;
    color: #000;
    padding: 0.5em 1.5em;
    line-height: 3em;
    text-transform: uppercase;
    font-weight: bold;
    &:hover {
      color: #000;
      cursor: pointer;
      text-decoration: none !important;
    }
  }
  &.narrow {
    max-width: 50%;
    margin-left: auto;
    margin-right: auto;
  }
  .narrow {
    max-width: 50%;
    margin-left: auto;
    margin-right: auto;
  }
  .narrow-medium {
    max-width: 70%;
    margin-left: auto;
    margin-right: auto;
  }
  
  @media (max-width: 1026px) {
    &.narrow {
      max-width: 80%;
    }
    .narrow {
      max-width: 80%;
    }
    .shipping-promo {
      flex-flow: wrap;
      &>div:first-child {
        flex: 0 0 100%;
      }
    }
    .reward {
      p:last-child {
        margin-bottom: 5px;
      }
      &>div {
        flex: 0 0 100%;
        margin-right: 0;
        margin-bottom: 10px;
        .fas {
          font-size: 40px;
        }
      }
    }
    .instruction {
      margin: 50px 0;
      flex-flow: column;
      div:first-child {
        flex: 100%;
        margin-left: 0px;
      }
    }
  }
  @media (max-width: 580px) {
    &.narrow {
      max-width: 100%;
    }
    .narrow {
      max-width: 100%;
    }
    .narrow-medium {
      max-width: 100%;
    }
  }
}
/*------------------------------------------------------------------
Glossary Page
