/*------------------------------------------------------------------
Video Layout
-------------------------------------------------------------------*/
.video-layout {
  display: flex;
  flex-flow: wrap;
  justify-content: space-around;
  p {
    color: #16142C;
    text-transform: uppercase;
    font-weight: bold;
  }
  &.no-video {
  	justify-content: space-between;
  }
  
  div {
    margin: 10px 0;
    flex-basis: calc(33% - 20px);
    iframe {
      height: 250px;
      width: 100%;
    }
    @media (max-width: 1200px) {
      flex-basis: calc(50% - 20px);
    }
    @media (max-width: 768px) {
      flex-basis: 100%;
      iframe {
        height: 400px;
        width: 100%;
      }
    }
  }
}

