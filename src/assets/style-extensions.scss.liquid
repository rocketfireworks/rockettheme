.product-container {
  position: relative;
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: auto 1fr;
  .product-gallery-container{
    grid-row: 1 / 3;
    align-self: center;
    .product-image-link {
      max-height: 300px;
    }
  }
  .product-details {
    grid-column: 2 / 3;
    grid-row: 1 / 2;
    padding: 10px 0 0 15px;
    .product-title-inventory {
      h1 {
        padding-right: 10px;
        font-size: 2.125em;
        font-weight: bold;
        text-transform: uppercase;
        color: #16142c;
      }
    }
  }
  .product-description {
    grid-row: 2 / 3;
    grid-column: 2 / 3;
    position: relative;
    padding: 0 0 0 15px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    .data-title {
      font-weight: bold;
    }
  }
  @media (max-width: 991px) {
    grid-template-columns: 1fr;
    row-gap: 10px;
    .product-details {
      grid-column: 1 / 2;
      grid-row-start: 1;
      grid-row-end: 2;
      display: grid;
      padding: 0;
      .product-title-inventory {
        grid-column: 1 / 2;
      }
      .product-price-qty {
        grid-column: 2 / 3;
        align-items: center;
        justify-content: flex-end;
        .price {
          text-align: right;
          flex-basis: 100%;
          #comparePrice {
            font-size: 1.25em;
          }
          #productPrice {
            font-size: 1.8em;
          }
        }
        .maximum-in-stock-error {
          bottom: -32px;
        }
      }
    }
    .product-gallery-container{
      grid-row: 2 / 3;
    }
    .product-sku-share {
      grid-column: 1 / 2;
      grid-row: 3 / 4;
    }
    .product-description {
      grid-row: 4 / 5;
      grid-column: 1 / 2;
      padding: 0;
    }
  }
  @media (max-width: 560px) {
    .product-details {
      font-size: 14px;
    }
  }

.product-price-qty {
  position: relative;
  display: flex;
  flex-wrap: wrap;
  align-items: flex-end;
  .price {
    line-height: 1.2;
    color: #282827;
    padding: 0;
    display: block;
    #comparePrice {
      color: #777;
      font-size: 1.25em;
      text-decoration: line-through;
    }
    #productPrice {
      font-size: 2.2em;
    }
  }
  .qty {
    position: relative;
    display: inline-flex;
    align-items: center;
    overflow: initial;
    padding-left: 10px;
  }
  .minus_btn,
  .plus_btn {
    display: block;
    width: 2.5em;
    height: 2.2em;
    border: 1px solid #ffffffff;
    border-radius: 0;
    cursor: pointer;
    &:hover {
      border: 1px solid #d7dbdb;
    }
  }
  .minus_btn {
    background: url(minus.png) no-repeat center center;
