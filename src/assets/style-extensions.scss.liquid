  }
}
/*------------------------------------------------------------------
Contact Page
-------------------------------------------------------------------*/
#contact-info {
  .contact-container {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-between;
    align-items: flex-start;
    p {
      color: #16142C;
    }
    li {
      color: #16142C;
    }
    .general-info {
      flex-basis: 50%;
    }
    .main-store {
      flex-basis: 45%;
      border: 0.5px solid grey;
      border-radius: 4px;
      &>h2 {
        color: white;
        background-color: #107fa8;
        margin: 0;
        padding: 10px;
        flex: 0 0 100%;
      }
      &>div {
        padding: 10px;
        display: flex;
        flex-flow: row wrap;
        justify-content: space-around;
        &>div {
        }
      }
    }
    .more-info {
      display: flex;
      justify-content: flex-start;
      flex-flow: row wrap;
      justify-content: space-around;
      margin-top: 40px;
      &>div {
        flex: 0 0 50%;
      }
    }
    .fas {
      color: #107fa8;
    }
    .fa-store, .fa-shipping-fast, .fa-birthday-cake, .fa-facebook-f {
      font-size: 40px;
      color: #107fa8;
    }
    .layout-inline {
      &>:first-child {
        width:10%;
        margin-right: 15px;
        text-align: center;
      }
    }
    @media (min-width: 580px) and (max-width: 1026px) {
      .general-info {
        flex: 0 0 100%;
      }
      .main-store {
        flex: 0 0 100%;
      }
      .more-info {
        &>div {
          flex: 0 0 100%;
        }
      }
      .layout-inline {
        &>:first-child {
          margin-right: 25px;
        }
      }
    }
    @media (max-width: 580px) {
      .general-info {
        flex: 0 0 100%;
      }
      .main-store {
        flex: 0 0 100%;
        &>div>div {
          flex: 0 0 100%;
          padding: 10px 8%;
        }
      }
      .more-info {
        &>div {
          flex: 0 0 100%;
        }
      }
      .layout-inline {
        &>:first-child {
          margin-right: 35px;
        }
      }
    }
  }
}


/*------------------------------------------------------------------
FAQ Page
-------------------------------------------------------------------*/
#fireworks-faq, #shipping-faq-online-orders {
  .page_content p {
    margin-left: 40px;
  }
  .page_content ul {
    margin-left: 40px;
    li {
      margin-left: 0;
      color: #16142C;
    }
  }
  .fa-question-circle, .fa-question {
    font-size: 35px;
    margin-right: 20px;
    color: #16142C;
  }
  .page_content h2 {
    margin-top: 30px;
    margin-bottom: 15px;
    display: flex;
    align-items: center;
  }
  .layout-inline {
    p {
      margin-left: 0;
      margin-bottom: 0;
    }
  }
  @media (max-width: 580px) {
  	.layout-inline {
      display: initial;
  		& :first-child {
  		  margin-right: 0;
          margin-bottom: 5px;
          text-align: center;
  		}
