    }
    @media (max-width: 580px) {
      .glossary-item {
        &>div {
          flex-flow: column;
        }
      }
    }
  }
}
/*------------------------------------------------------------------
Policy Page
-------------------------------------------------------------------*/
#return-policy, #privacy-policy {
  .page_content h2 {
    margin-top: 30px;
  }
  .page_content h3 {
    margin-top: 20px
  }
